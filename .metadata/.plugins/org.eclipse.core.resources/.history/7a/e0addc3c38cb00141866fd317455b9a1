#include <iostream>
#include <atop/fem/define_mesh.h>

using namespace atop;

int main(){

	//Define the mesh
	DefineMesh<2> mesh;
	mesh.coordinates = {{0, 2}, {0, 1}};
	mesh.showdata(); // this function is not there in the cpp file
	std::cout<<"Completed successfully"<<std::endl;
}
