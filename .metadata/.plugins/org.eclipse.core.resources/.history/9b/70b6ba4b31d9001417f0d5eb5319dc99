#include <string>
#include <atop/fem/define_mesh.h>

using namespace atop;

//Constructor for getting only number of physical dimensions
DefineMesh::DefineMesh(int dim){
	this->dim = dim;
	input_file = "";
}

void DefineMesh::showdata(){
	source_fn({2.0, 2.0});
}
