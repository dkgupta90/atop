#include <atop/TopologyOptimization/optimizedesign.h>
#include <atop/fem/define_mesh.h>
#include <atop/TopologyOptimization/penalization.h>

using namespace atop;

DefineMesh mesh;
Penalize penal;

Optimizedesign::Optimizedesign(
		DefineMesh &mesh,
		Penalize &penal
		){
	this->mesh = mesh;
	this->penal = penal;

}




